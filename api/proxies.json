{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "users": {
      "debug": false,
      "matchCondition": {
        "route": "/api/users/{*restOfPath}"
      },
      "backendUri": "%users_url%/api/users/{restOfPath}",
      "requestOverrides": {
        "backend.request.headers.Authorization": "{request.headers.x-authorization}"
      }
    },
