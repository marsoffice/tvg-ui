{
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "routes": [
    {
      "route": "/.auth/*",
      "statusCode": 404
    },
    {
      "route": "/index.html",
      "headers": {
        "Cache-Control": "no-cache"
      }
    },
    {
      "route": "/ngsw.json",
      "headers": {
        "Cache-Control": "no-cache"
      }
    },
    {
      "route": "/ngsw-worker.js",
      "headers": {
        "Cache-Control": "no-cache"
      }
    }
  ]
}
